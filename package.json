{
  "name": "update",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "eslint": "eslint . --ext .js,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "stylelint": "stylelint 'src/**/*.css'",
    "type-check": "tsc --pretty --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn eslint",
      "yarn stylelint",
      "yarn format"
    ]
  },
  "dependencies": {
    "autoprefixer": "^10.1.0",
    "dotenv": "^8.2.0",
    "gatsby": "^2.28.2",
    "gatsby-awesome-pagination": "^0.3.6",
    "gatsby-image": "^2.7.0",
    "gatsby-plugin-google-analytics": "^2.7.0",
    "gatsby-plugin-manifest": "^2.8.0",
    "gatsby-plugin-offline": "^3.6.0",
    "gatsby-plugin-postcss": "^3.3.0",
    "gatsby-plugin-react-helmet": "^3.6.0",
    "gatsby-plugin-sharp": "^2.10.1",
    "gatsby-plugin-sitemap": "^2.8.0",
    "gatsby-plugin-typegen": "^2.2.1",
    "gatsby-remark-images": "^3.7.1",
    "gatsby-remark-images-contentful": "^2.6.0",
    "gatsby-remark-prismjs": "^3.9.0",
    "gatsby-source-contentful": "^4.2.1",
    "gatsby-source-filesystem": "^2.7.0",
    "gatsby-transformer-remark": "^2.12.0",
    "gatsby-transformer-sharp": "^2.8.0",
    "postcss": "^8.2.1",
    "prismjs": "^1.22.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.12",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-tailwind": "^0.2.0",
    "husky": "^4.3.5",
    "lint-staged": "^10.5.3",
    "prettier": "2.2.1",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-order": "^4.1.0",
    "tailwindcss": "^2.0.2",
    "typescript": "^4.1.2"
  }
}
