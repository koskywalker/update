{
  "name": "update",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "develop:clean": "gatsby clean && gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "eslint": "eslint . --ext .js,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "stylelint": "stylelint 'src/**/*.css'",
    "type-check": "tsc --pretty --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn eslint",
      "yarn stylelint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^0.2.0",
    "@tailwindcss/typography": "^0.3.1",
    "dayjs": "^1.9.8",
    "dotenv": "8.2.0",
    "gatsby": "2.29.1",
    "gatsby-awesome-pagination": "0.3.6",
    "gatsby-image": "2.8.0",
    "gatsby-plugin-google-analytics": "2.8.0",
    "gatsby-plugin-manifest": "2.9.0",
    "gatsby-plugin-offline": "3.7.0",
    "gatsby-plugin-postcss": "3.4.0",
    "gatsby-plugin-react-helmet": "3.7.0",
    "gatsby-plugin-sharp": "2.11.1",
    "gatsby-plugin-sitemap": "2.9.0",
    "gatsby-plugin-typegen": "2.2.2",
    "gatsby-remark-autolink-headers": "^2.9.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "3.8.0",
    "gatsby-remark-images-contentful": "2.7.0",
    "gatsby-remark-prismjs": "3.10.0",
    "gatsby-remark-table-of-contents": "^1.0.0",
    "gatsby-source-contentful": "4.3.0",
    "gatsby-source-filesystem": "2.8.0",
    "gatsby-transformer-remark": "2.13.0",
    "gatsby-transformer-sharp": "2.9.0",
    "prismjs": "1.22.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-tsparticles": "^1.18.11",
    "typed.js": "^2.0.11"
  },
  "devDependencies": {
    "@types/node": "14.14.14",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-helmet": "6.1.0",
    "@typescript-eslint/eslint-plugin": "4.11.0",
    "@typescript-eslint/parser": "4.11.0",
    "autoprefixer": "10.1.0",
    "eslint": "7.16.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-tailwind": "0.2.0",
    "husky": "4.3.6",
    "lint-staged": "10.5.3",
    "postcss": "8.2.1",
    "prettier": "2.2.1",
    "stylelint": "13.8.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.1.0",
    "tailwindcss": "2.0.2",
    "typescript": "4.1.3"
  }
}
